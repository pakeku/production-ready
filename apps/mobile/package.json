{
  "name": "mobile",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "expo start --web",
    "build": "pnpm build:local:android && pnpm build:local:ios",
    "build:dev:ios": "npx eas-cli build --profile development --platform ios",
    "build:dev:android": "npx eas-cli build --profile development --platform android",
    "build:local:ios": "npx eas-cli build --profile development --platform ios --local",
    "build:local:android": "npx eas-cli build --profile development --platform android --local",
    "build:preview": "npx eas-cli build --profile preview --platform all",
    "build:preview:ios": "npx eas-cli build --profile preview --platform ios",
    "build:preview:android": "npx eas-cli build --profile preview --platform android",
    "build:prod": "npx eas-cli build --profile production --platform all",
    "build:prod:ios": "npx eas-cli build --profile production --platform ios",
    "build:prod:android": "npx eas-cli build --profile production --platform android",
    "submit:ios": "npx eas-cli submit --platform ios",
    "submit:android": "npx eas-cli submit --platform android",
    "update": "npx eas-cli update",
    "update:preview": "npx eas-cli update --branch preview",
    "update:production": "npx eas-cli update --branch production",
    "export:web": "npx expo export --platform web"
  },
  "dependencies": {
    "@repo/core": "workspace:*",
    "@repo/ui": "workspace:*",
    "expo": "~54.0.33",
    "expo-dev-client": "~6.0.20",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.5",
    "react-native-svg": "^15.12.1",
    "tamagui": "^1.98.0"
  },
  "devDependencies": {
    "@tamagui/babel-plugin": "1.138.5"
  }
}
